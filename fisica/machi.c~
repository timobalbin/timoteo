#include <stdio.h>
int main(){


}
double tiempo(double dx, double v){
	return dx/v;
}

double velocidad(double dx, double dt){
	return dx/dt;
}

double distancia(double v, double dt){
	return v*dt;
}

double Vox(double Vo, double alpha)
	return Vo*cos alpha




double degreesToRadianes(double degree)	
	return degree*3.14/180;
